    <steps>
      <step>
        <cmd>Process the abstract values (MARC<codeph>520a</codeph>).</cmd>
        <info>
          <table>
            <title>JSON values for main titles</title>
            <tgroup cols="3">
              <thead>
                <row>
                  <entry>JSON structure</entry>
                  <entry>Description</entry>
                  <entry>Default</entry>
                </row>
              </thead>
              <tbody>
                <row>
                  <entry>
                    <codeph>root → referred_to_by → type</codeph>
                  </entry>
                  <entry/>
                  <entry>
                    <codeph>LinguisticObject</codeph>
                  </entry>
                </row>
                <row>
                  <entry>
                    <codeph>root → referred_to_by → content</codeph>
                  </entry>
                  <entry>String value of MARC<codeph>520a</codeph>subfield</entry>
                  <entry/>
                </row>
                <row>
                  <entry>
                    <codeph>root → referred_to_by → classified_as → id</codeph>
                  </entry>
                  <entry>IRI of the concept used to classify the statement</entry>
                  <entry>
                    <xref format="html" href="http://vocab.getty.edu/aat/300026032" scope="external"/>
                  </entry>
                </row>
                <row>
                  <entry>
                    <codeph>root → referred_to_by → classified_as → type</codeph>
