  <taskbody>
    <context id="context_pwz_rvj_5rb">Apply these instructions for source data fields that reference a single entity.</context>
    <section id="section_qwz_rvj_5rb">
      <title>Source data</title>
      <codeblock id="codeblock_rwz_rvj_5rb">---
name: SingleEntities
sampleBibs:
  - 3 # 650
  - 7 # 710
  - 9 # 710
  - 11 # 110
  - 19 # 111
  - 38 # 110
  - 68 # 610
  - 127 # 111
  - 162 # 610
  - 422 # 693
  - 426 # 711
  - 607 # 611
  - 693 # 100, 700
  - 947 # 711
  - 1029 # 611
  - 8839 # 611
  - 9292 # 693
  - 4504 # 655
  - 614880 # 694
  - 838469 # 694
  - 907221 # 600
  - 1066178 # 751
  - 6017172 # 651
# Source data fields
fieldSpec:
  - 100014abcdegjq
  - 110014abcdeg
  - 111014cdegjnqu
  - 600014abcdegjq
  - 610014abcdeg
  - 611014acdegjnqu
  - 65004abcdeg
  - 651| 0|014ae
  - 655014abc
